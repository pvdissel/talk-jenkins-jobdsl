{
    "fullName": "Something AAB",
    "flow": {
        "deploy": ["nightly"]
    },
    "pipelines": [
        {
            "type": "APP",
            "tool": {
                "type": "maven",
                "version": "3.1.1"
            },
            "technology": {
                "type": "jdk",
                "version": "6u45"
            },
            "scm": {
                "type": "svn",
                "base": "https://svn.company.tld/group/something-aab-service"
            },
            "ci": {
                "maven": {
                    "rootpom": "pom.xml",
                    "goals": "clean cobertura:clean compile dependency:list install pmd:pmd pmd:cpd findbugs:findbugs cobertura:cobertura",
                    "opts": "-DoutputFile=dependencies.txt"
                },
                "cobertura": {
                    "coberturaReportFile": "**/target/site/cobertura/coverage.xml",
                    "onlyStable": "false",
                    "failUnhealthy": "false",
                    "failUnstable": "false",
                    "autoUpdateHealth": "false",
                    "autoUpdateStability": "false",
                    "zoomCoverageChart": "false",
                    "failNoReports": "false",
                    "sourceEncoding": "ASCII",
                    "targets": [
                        {
                            "type": "healthy",
                            "method": "70",
                            "line": "70",
                            "conditional": "60"
                        },
                        {
                            "type": "unhealthy",
                            "method": "50",
                            "line": "50",
                            "conditional": "40"
                        },
                        {
                            "type": "failing",
                            "method": "60",
                            "line": "60",
                            "conditional": "50"
                        }
                    ]
                },
                "dry": {
                    "healthy": "",
                    "unhealthy": "",
                    "thresholdLimit": "low",
                    "defaultEncoding": "",
                    "canRunOnFailed": "",
                    "useStableBuildAsReference": "false",
                    "useDeltaValues": "",
                    "thresholds": {
                        "unstableTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "unstableNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        }
                    },
                    "shouldDetectModules": "",
                    "dontComputeNew": "",
                    "doNotResolveRelativePaths": "",
                    "highThreshold": "50",
                    "normalThreshold": "25"
                },
                "pmd": {
                    "healthy": "5",
                    "unhealthy": "",
                    "thresholdLimit": "normal",
                    "defaultEncoding": "",
                    "canRunOnFailed": "",
                    "useStableBuildAsReference": "false",
                    "useDeltaValues": "",
                    "thresholds": {
                        "unstableTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "unstableNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        }
                    },
                    "shouldDetectModules": "",
                    "dontComputeNew": "",
                    "doNotResolveRelativePaths": ""
                },
                "tasks": {
                    "healthy": "10",
                    "unhealthy": "",
                    "thresholdLimit": "normal",
                    "defaultEncoding": "",
                    "canRunOnFailed": "",
                    "useStableBuildAsReference": "false",
                    "useDeltaValues": "",
                    "thresholds": {
                        "unstableTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "unstableNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        }
                    },
                    "shouldDetectModules": "",
                    "dontComputeNew": "",
                    "doNotResolveRelativePaths": "",
                    "pattern": "**/*.java",
                    "excludePattern": "",
                    "high": "FIXME",
                    "normal": "TODO",
                    "low": "",
                    "ignoreCase": "true"
                },
                "locks": []
            },
            "deploy": {
                "publishedArtifacts": ["target/aab-dc/**/*=>tld/company/aab"],
                "params": ["-p aab-parent"]
            }
        }
    ]
}
